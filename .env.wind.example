# Wind API 配置示例
# 复制此文件为 .env.local 并填入实际的Wind API凭证

# Wind API 服务地址
WIND_API_URL=http://localhost:8080

# Wind API 认证信息
WIND_USERNAME=your_wind_username
WIND_PASSWORD=your_wind_password

# Wind API 超时设置（毫秒）
WIND_TIMEOUT=30000

# Wind API 缓存设置
WIND_CACHE_EXPIRE=300000  # 5分钟缓存

# Wind API 批量查询限制
WIND_BATCH_LIMIT=50  # 单次最多查询50个证券

# 是否启用Wind API（true/false）
WIND_ENABLED=true